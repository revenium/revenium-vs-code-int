{
  "typescript.preferences.importModuleSpecifier": "relative",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.exclude": {
    "node_modules": true,
    "dist": true,
    "*.vsix": true
  },
  "search.exclude": {
    "node_modules": true,
    "dist": true
  },
  "typescript.tsc.autoDetect": "on",
  "eslint.validate": ["javascript", "typescript"],
  "revenium.detectionActive": true,
  "revenium.operationMode": "guide",
  "revenium.patterns.openai.enabled": true,
  "revenium.patterns.anthropic.enabled": false,
  "revenium.patterns.google.enabled": false,
  "revenium.integrations.middleware.enabled": true,
  "revenium.integrations.async.enabled": false,
  "revenium.patterns.advanced.enabled": true,
  "revenium.detectionFilter": "imports",
  "revenium.providers.openai": true,
  "revenium.providers.anthropic": true,
  "revenium.providers.google": true,
  "revenium.advancedPatterns": true,
  "revenium.languages.javascript": true,
  "revenium.languages.typescript": true,
  "revenium.integrationFocus.async": true,
  "revenium.enableDebugOutput": false,
  "revenium.languages.python": true
}
